{
  "name": "helloworld",
  "version": "0.0.1",
  "description": "Electron Auto Updater Demo",
  "main": "main.js",
  "author": "applecool",
  "scripts": {
    "start": "electron .",
    "mac": "electron-builder -m",
    "build": "electron-builder -mwl"
  },
  "build": {
    "appId": "com.applecool.electron",
    "directories": {
      "buildResources": "."
    },
    "mac": {
      "publish": [
        "github"
      ],
      "category": "public.app-category.productivity"
    }
  },
  "dependencies": {
    "electron-log": "2.2.17",
    "electron-updater": "4.0.4",
    "query-string": "6.2.0"
  },
  "devDependencies": {
    "electron": "3.0.10",
    "electron-builder": "20.36.2"
  }
}
